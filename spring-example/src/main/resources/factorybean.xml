<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="sessionFactory" class="com.chuhui.blazers.springexample.factorybean.CustomDataSessionFactoryBean">
        <constructor-arg name="password" value="password-xml"/>
        <constructor-arg name="url" value="url-xml"/>
        <constructor-arg name="user" value="user-xml"/>
    </bean>

</beans>